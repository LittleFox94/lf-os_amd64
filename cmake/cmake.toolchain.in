set(CMAKE_SYSTEM_NAME LF-OS)
set(CMAKE_SYSTEM_PROCESSOR x86-64)
set(CMAKE_MODULE_PATH "${CMAKE_MODULE_PATH};@toolchain@/share/cmake/Modules")

set(CMAKE_SYSROOT @toolchain@)

set(CMAKE_C_COMPILER   @toolchain@/bin/clang)
set(CMAKE_C_COMPILER_TARGET x86_64-pc-lf_os)
set(CMAKE_CXX_COMPILER @toolchain@/bin/clang++)
set(CMAKE_CXX_COMPILER_TARGET x86_64-pc-lf_os)
